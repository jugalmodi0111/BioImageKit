   "source": [
    "import numpy as np, matplotlib.pyplot as plt\n",
    "from skimage import data, metrics\n",
    "img = data.camera(); noisy = np.clip(img.astype(float)+10*np.random.randn(*img.shape),0,255).astype('uint8')\n",
    "psnr = metrics.peak_signal_noise_ratio(img, noisy)\n",
    "print('PSNR (dB):', round(psnr,2))"
   ]
